{
    "editor.wordWrap": "on",
    // "editor.fontSize": 16,
    // "window.zoomLevel": -1,
    "workbench.colorTheme": "Big Bang -> Pitch Black Theme",
    // "terminal.integrated.fontSize": 15,
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorCustomizations": {
      "activityBar.foreground": "#ffae00",
      "activityBarBadge.background": "#5ff001",
      "activityBarBadge.foreground": "#0f0f0f",
      // Border separating the title bar from the rest of the editor
      "titleBar.border": "#ffae00",
      // Border color to separate Tabs from each other
      "tab.border": "#ffae00",
      // Border color on the top separating to the editor (terminal)
      "panel.border": "#ffae00",
      // Border color on the side separating the editor
      "sideBar.border": "#ffae00",
      // Border color with the Side Bar
      "activityBar.border": "#ffae00",
      // Border color separating the Status Bar and editor
      "statusBar.border": "#ffae00",
      // Border color to separate multiple editor groups from each other
      "editorGroup.border": "#ffae00",
      // Border color of the editor group title header when tabs are enabled
      "editorGroupHeader.tabsBorder": "#ffae00"
    },
    // "editor.tokenColorCustomizations": {
    //     "comments": "#ffea00",
    //     "keywords": "#5ff001",
    //     "strings": "#ff00ea",
    //     "types": "#00ff22",
